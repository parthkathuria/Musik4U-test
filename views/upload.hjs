                                                    <form class="form-horizontal" id="uploadForm" enctype="multipart/form-data" action="/wall/{{sessionId}}/audio" method="POST">
                                                        <div class="form-group">
                                                            <label class="col-lg-2 control-label">Title</label>
                                                            <div class="col-lg-10">
                                                                <input type="text" name="title" id="title" class="form-control" required placeholder="Title...">
                                                            </div>
                                                        </div>
                                                        <div class="form-group">
                                                            <label class="col-lg-2 control-label">Artist</label>
                                                            <div class="col-lg-10">
                                                                <input type="text" id="artist" name="artist" class="form-control" required placeholder="Artist...">
                                                            </div>
                                                        </div>
                                                        
                                                        <div class="form-group">
                                                            <label class="col-lg-2 control-label">Genre</label>
                                                            <div class="col-lg-10">
                                                                <select name="genre" id="genre" class="form-control m-b" required>
                                                                    <option value="0">--Select Genre--</option>
                                                                    <option value="1">acoustic</option>
                                                                    <option value="2">ambient</option>
                                                                    <option value="3">blues</option>
                                                                    <option value="4">classical</option>
                                                                    <option value="5">country</option>
                                                                    <option value="6">electronic</option>
                                                                    <option value="7">emo</option>
                                                                    <option value="8">folk</option>
                                                                    <option value="9">hardcore</option>
                                                                    <option value="10">hip hop</option>
                                                                    <option value="11">indie</option>
                                                                    <option value="12">jazz</option>
                                                                    <option value="13">latin</option>
                                                                    <option value="14">metal</option>
                                                                    <option value="15">pop</option>
                                                                    <option value="16">pop punk</option>
                                                                    <option value="17">punk</option>
                                                                    <option value="18">reggae</option>
                                                                    <option value="19">rnb</option>
                                                                    <option value="20">rock</option>
                                                                    <option value="21">soul</option>
                                                                    <option value="22">world</option>
                                                                </select>
                                                            </div>
                                                        </div>
                                                         <div class="form-group">
                                                            <label class="col-lg-2 control-label">Description</label>
                                                            <div class="col-lg-10">
                                                                <textarea type="text" id="description" name="description" rows="4" cols="50" class="form-control" placeholder="Description..."></textarea>
                                                            </div>
                                                        </div>
                                                        <div class="form-group">
                                                            <label class="col-lg-2 control-label">Select Art</label>
                                                            <div class="col-lg-10">
                                                                <input type="file" name="albumArt" id="albumArt" class="btn btn-default">
                                                            </div>
                                                        </div>
                                                        <div class="form-group">
                                                            <label class="col-lg-2 control-label">Select File</label>
                                                            <div class="col-lg-10">
                                                                <input type="file" name="audioFile" id="audioFile" class="btn btn-default" required>
                                                            </div>
                                                        </div>
                                                        <div class="form-group">
                                                            <!--<a href="#" class="btn btn-dark" data-dismiss="modal">Close</a>-->
                                                            <div class="col-lg-offset-2 col-lg-10">
                                                                <button onClick="upload_file()" class="btn btn-success" data-toggle="class:show inline" data-target="#spin" data-loading-text="Uploading..."><i class="fa fa-cloud-upload text"></i> Upload</button> <i class="fa fa-spin fa-spinner hide" id="spin"></i>
                                                            </div>
                                                        </div>
                                                    </form>
                                                </div>
                                            </section>
                                        </div>
                                        
                                    </div>
